  },
  "jest": {
    "collectCoverageFrom": [
      "server/*.js",
      "client/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 25
      }
    }
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
