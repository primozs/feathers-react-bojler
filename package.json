{
  "name": "feathers-react-bojler",
  "description": "React boiler app",
  "version": "1.0.1",
  "homepage": "",
  "main": "server/index.js",
  "keywords": [
    "feathers"
  ],
  "license": "MIT",
  "repository": {},
  "author": "Primoz Susa <primoz.susa@niceclouds.com> (niceclouds.com)",
  "contributors": [],
  "bugs": {},
  "scripts": {
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "start": "NODE_ENV=development nodemon --debug=5858 server/index.js",
    "clean": "rm -rf dist && mkdir dist && npm run copy:static && npm run copy:config",
    "copy:static": "rsync -rtvuc --delete static/ dist/static/",
    "copy:config": "rsync -rtvuc --delete config/ dist/config/",
    "build:server": "NODE_ENV=production babel server -d dist/server",
    "build:client_dir": "NODE_ENV=production babel client -d dist/client",
    "build:client": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:client:proddev": "NODE_ENV=production webpack --config webpack.config.proddev.js",
    "build": "npm run clean && npm run build:server && npm run build:client_dir && npm run build:client",
    "build:proddev": "npm run clean &&  npm run build:server && npm run build:client_dir && npm run build:client:proddev",
    "start:prod": "NODE_ENV=production node dist/server/start.js",
    "build:start": "npm run build && npm run start:prod",
    "stats:dev": "webpack --config webpack.config.dev.js --profile --json > stats.dev.json",
    "stats:proddev": "webpack --config webpack.config.proddev.js --profile --json > dist/stats.proddev.json",
    "stats:proddev:analyze": "analyze-bundle -b ./dist/static/app.5e58490e5c80f7268dce.js -s ./dist/static/stats.json",
    "stats:proddev:electrify": "npm run stats:proddev && electrify ./dist/stats.proddev.json --open",
    "stats:proddev:sources": "source-map-explorer ./dist/static/app.5e58490e5c80f7268dce.js ./dist/static/app.01966c2c9eea9f914836.js.map",
    "slate": "rm -rf node_modules && yarn install",
    "push": "git push origin && git push origin --tags",
    "lint": "eslint client server",
    "lint:watch": "esw -w client server"
  },
